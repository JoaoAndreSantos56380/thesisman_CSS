<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ul.fc.di.css.javafxexample.presentation.control.MainController">
    <top>
        <HBox spacing="10">
            <Button fx:id="studentButton" text="Students" />
            <Button fx:id="professorButton" text="Professors" />
            <Button fx:id="masterButton" text="Masters" />
        </HBox>
    </top>
    <center>
        <StackPane fx:id="contentPane" />
    </center>
    <right>
        <StackPane fx:id="rightPane" />
    </right>
</BorderPane>
